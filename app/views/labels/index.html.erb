<h1><%= t('view_title.models.label.index') %></h1>

<table>
  <thead>
    <tr>
      <th><%= t('activerecord.attributes.label.name') %></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @labels.each do |label| %>
      <tr>
        <td><%= label.name %></td>
        <td><%= link_to t('helpers.select.show'), label %></td>
        <td><%= link_to t('helpers.select.edit'), edit_label_path(label) %></td>
        <td><%= link_to t('helpers.select.destroy'), label, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to t('view_title.models.label.new'), new_label_path %>
<br>
<%= link_to t('view_title.models.admin_user.index'), admin_users_path %>
